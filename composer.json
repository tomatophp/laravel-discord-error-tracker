{
    "name": "tomatophp/laravel-discord-error-tracker",
    "type": "library",
    "description": "Track Your Errors using Discord Webhook on any Laravel App",
    "keywords": [
        "php",
        "laravel",
        "discord",
        "error-tracker",
        "error",
        "tracker",
        "logger",
        "discord-webhook"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\LaravelDiscordErrorTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomatoPHP\\LaravelDiscordErrorTracker\\Tests\\": "tests/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\LaravelDiscordErrorTracker\\LaravelDiscordErrorTrackerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "testbench": "vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse src tests",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "require": {
        "php": "^8.1|^8.2",
        "laravel/framework": "*",
        "guzzlehttp/guzzle": "*"
    },
    "require-dev": {
       "laravel/pint": "^1.18",
       "livewire/livewire": "^2.10|^3.0",
       "nunomaduro/larastan": "^2.9",
       "orchestra/testbench": "^9.5",
       "pestphp/pest": "^2.36",
       "pestphp/pest-plugin-laravel": "^2.4",
       "pestphp/pest-plugin-livewire": "^2.1",
       "phpstan/extension-installer": "^1.4",
       "phpstan/phpstan-deprecation-rules": "^1.2",
       "phpstan/phpstan-phpunit": "^1.4"
    },
    "version": "v1.0.0"
}
